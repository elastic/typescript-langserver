{
  "name": "@elastic/typescript-langserver",
  "version": "0.0.3",
  "private": false,
  "scripts": {
    "start": "node ./lib/cli.js",
    "watch": "tsc -b --watch --verbose",
    "build": "yarn && yarn compile",
    "compile": "tsc -b",
    "lint": "yarn typecheck && tslint -c ./tslint.yaml -p ./tsconfig.json",
    "typecheck": "tsc -p ./tsconfig.json --noEmit",
    "pub": "yarn build && yarn publish:latest"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "@elastic/lsp-extension": "^0.1.1",
    "@elastic/typescript-language-server": "^0.3.8-alpha3",
    "child-process-promise": "^2.2.1",
    "mz": "^2.7.0",
    "npm-registry-fetch": "^3.8.0",
    "semver": "^5.6.0",
    "source-map": "^0.7.3",
    "typescript": "^3.3.4000",
    "yarn": "^1.12.3"
  },
  "devDependencies": {
    "@types/child-process-promise": "^2.2.1",
    "@types/fs-extra": "^5.0.4",
    "@types/mz": "0.0.32",
    "@types/node": "10.12.18",
    "@types/p-debounce": "^1.0.0",
    "@types/rimraf": "^2.0.2",
    "@types/semver": "5.5.0",
    "tslint": "^5.11.0",
    "typescript-tslint-plugin": "^0.3.1"
  }
}
